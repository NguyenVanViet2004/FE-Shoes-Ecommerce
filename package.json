    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:diff": "jest --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "lint": "eslint --ext js,jsx,ts,tsx . --ignore-path .gitignore",
    "lint:fix": "eslint --ext js,jsx,ts,tsx . --fix --ignore-path .gitignore",
    "postinstall": "husky install",
    "prepare": "husky install",
    "distributeIos": "eas build --platform ios --profile preview",
    "distributeAndroid": "eas build -p android --profile preview",
    "buildIos": "eas build -p ios"
  },
